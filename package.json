{
	"name": "skiftesgatan",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:node": "PUBLIC_ADAPTER=node vite build",
		"build:static": "PUBLIC_ADAPTER=static vite build",
		"build:cap": "pnpm build:static && pnpm cap sync",
		"preview": "vite preview",
		"preview:node": "PUBLIC_ADAPTER=node vite preview --outDir build-node",
		"preview:static": "PUBLIC_ADAPTER=static vite preview --outDir build-static",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"typegen": "pocketbase-typegen --env --out src/lib/pocketbase-types.ts && prettier --write src/lib/pocketbase-types.ts",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"devDependencies": {
		"@capacitor/android": "^5.7.0",
		"@capacitor/app": "^5.0.7",
		"@capacitor/cli": "^5.7.0",
		"@capacitor/core": "^5.7.0",
		"@capacitor/device": "^5.0.7",
		"@capacitor/haptics": "^5.0.7",
		"@capacitor/ios": "^5.7.0",
		"@capacitor/preferences": "^5.0.7",
		"@capacitor/push-notifications": "^5.1.1",
		"@capacitor/splash-screen": "^5.0.7",
		"@fontsource-variable/lora": "^5.0.13",
		"@fontsource-variable/nunito-sans": "^5.0.9",
		"@internationalized/date": "^3.5.2",
		"@melt-ui/pp": "^0.3.0",
		"@melt-ui/svelte": "^0.74.0",
		"@sentry/sveltekit": "^7.101.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"@vite-pwa/assets-generator": "^0.2.4",
		"autoprefixer": "^10.4.17",
		"bits-ui": "^0.18.1",
		"clsx": "^2.1.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"pocketbase": "^0.21.1",
		"pocketbase-typegen": "^1.2.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"radix-icons-svelte": "^1.2.1",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.2.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.3"
	},
	"type": "module"
}
